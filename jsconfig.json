{
    "compilerOptions": {
        "checkJs": true,
        "allowSyntheticDefaultImports": true,
        "paths": {
            "@services": ["./services"],
            "@services/*": ["./services/*"],
            "@utils/*": ["./utils/*"],
            "@models/*": ["./models/*"],
            "@consts/*": ["./consts/*"]
        }
    },
    "exclude": ["node_modules"]
}
