{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "lib": [
            "ES2021.String"
        ],
        "noImplicitAny": false,
        "moduleResolution": "node",
        "allowJs": true,
        "checkJs": true,
        "paths": {
            "@lib/*": [
                "./lib/*"
            ],
            "@model/*": [
                "./model/*"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}
