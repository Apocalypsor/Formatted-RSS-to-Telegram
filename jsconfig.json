{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2021",
        "noImplicitAny": false,
        "moduleResolution": "node",
        "allowJs": true,
        "checkJs": true,
        "paths": {
            "@services": ["./services"],
            "@services/*": ["./services/*"],
            "@utils/*": ["./utils/*"],
            "@models/*": ["./models/*"],
            "@consts/*": ["./consts/*"]
        }
    },
    "exclude": ["node_modules"]
}
