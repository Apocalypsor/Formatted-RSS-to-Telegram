{
    "name": "formatted-rss-to-telegram",
    "version": "0.0.1",
    "description": "基于Github Action的高度可定制化Telegram通知推送服务",
    "scripts": {
        "prisma:generate": "prisma generate",
        "prisma:migrate:dev": "prisma migrate dev",
        "prisma:migrate:deploy": "prisma migrate deploy",
        "build": "bun run scripts/build.ts",
        "start": "bun run dist/index.js",
        "dev": "NODE_ENV=development prisma migrate deploy && bun --watch ./src/index.ts",
        "lint": "eslint \"src/**/*.ts\"",
        "lint:fix": "eslint \"src/**/*.ts\" --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Apocalypsor/Formatted-RSS-to-Telegram.git"
    },
    "author": "Apocalypsor",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/Apocalypsor/Formatted-RSS-to-Telegram/issues"
    },
    "homepage": "https://github.com/Apocalypsor/Formatted-RSS-to-Telegram#readme",
    "dependencies": {
        "@prisma/client": "^5.22.0",
        "axios": "^1.13.2",
        "jsdom": "^20.0.3",
        "node-schedule": "^2.1.1",
        "nunjucks": "^3.2.4",
        "rss-parser": "^3.13.0",
        "socks-proxy-agent": "^8.0.5",
        "winston": "^3.19.0",
        "winston-daily-rotate-file": "^4.7.1",
        "yaml": "^2.8.2",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/bun": "latest",
        "@types/jsdom": "^21.1.7",
        "@types/node": "^22.19.3",
        "@types/node-schedule": "^2.1.8",
        "@types/nunjucks": "^3.2.6",
        "@types/xml2js": "^0.4.14",
        "eslint": "^9.39.2",
        "prettier": "^3.7.4",
        "prisma": "^5.22.0",
        "typescript-eslint": "^8.50.0"
    }
}
